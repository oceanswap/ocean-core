{
  "name": "ocean-core",
  "version": "0.1.0",
  "description": "Ocean Core",
  "repository": "https://github.com/oceanswap/ocean-core",
  "license": "MIT",
  "scripts": {
    "compile": "npx hardhat compile --force",
    "deploy:dev": "npx hardhat run scripts/deploy.js --network hardhat",
    "deploy:huobi": "npx hardhat run scripts/deploy.js --network huobi",
    "deploy:reopen": "npx hardhat run scripts/goon.js --network huobi",
    "deploy:deployLottery": "npx hardhat run scripts/deployedLottery.js --network huobi",
    "deploy:lotteryEnterDraw": "npx hardhat run scripts/lotteryTimerEnterDraw.js --network huobi",
    "deploy:lotteryDraw": "npx hardhat run scripts/lotteryTimerDraw.js --network huobi",
    "deploy:lotteryDrawCheat": "npx hardhat run scripts/lotteryTimerDrawCheat.js --network huobi",
    "deploy:lotteryReset": "npx hardhat run scripts/lotteryTimerReset.js --network huobi",
    "deploy:ido": "npx hardhat run ../ocean-core/scripts/deployIDO.js --network huobi_ido",

    "test": "npx hardhat run scripts/test.js --network huobi_ido"

  },
  "devDependencies": {
    "@nomiclabs/hardhat-ethers": "^2.0.1",
    "@nomiclabs/hardhat-waffle": "^2.0.1",
    "chai": "^4.3.0",
    "ethereum-waffle": "^3.3.0",
    "ethers": "^5.0.31",
    "hardhat": "^2.0.11"
  },
  "dependencies": {
    "@openzeppelin/contracts": "^3.3.0"
  }
}
